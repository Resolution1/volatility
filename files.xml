<?xml version="1.0" encoding="utf-8"?>
<assembly>
  <id>vc-all</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>tools/windows/winpmem/objchk_wnet_amd64/amd64</directory>
      <outputDirectory>/bin/drivers/chk/x64</outputDirectory>
      <includes>
        <include>winpmem.sys</include>
        <include>winpmem.pdb</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>tools/windows/winpmem/objfre_wnet_amd64/amd64</directory>
      <outputDirectory>/bin/drivers/fre/x64</outputDirectory>
      <includes>
        <include>winpmem.sys</include>
        <include>winpmem.pdb</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>tools/windows/winpmem/objchk_wnet_x86/i386</directory>
      <outputDirectory>/bin/drivers/chk/x86</outputDirectory>
      <includes>
        <include>winpmem.sys</include>
        <include>winpmem.pdb</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>tools/windows/winpmem/objfre_wnet_x86/i386</directory>
      <outputDirectory>/bin/drivers/fre/x86</outputDirectory>
      <includes>
        <include>winpmem.sys</include>
        <include>winpmem.pdb</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>target/bin</directory>
      <outputDirectory>/bin</outputDirectory>
      <includes>
        <include>**/winpmem.exe</include>
        <include>**/winpmem.pdb</include>
      </includes>
    </fileSet>
    <!--rekall uses only 32 bit version of winpmem driver-->
    <fileSet>
      <directory>target/bin/vc100/Release.x64</directory>
      <outputDirectory>/bin/vc100/x64/admemoryanalysis</outputDirectory>
      <includes>
        <include>**/winpmem.exe</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>target/bin/vc100/Release.Win32</directory>
      <outputDirectory>/bin/vc100/Win32/admemoryanalysis</outputDirectory>
      <includes>
        <include>**/winpmem.exe</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>dist/x64/admemoryanalysis</directory>
      <outputDirectory>/bin/vc100/x64/admemoryanalysis</outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>dist/Win32/admemoryanalysis</directory>
      <outputDirectory>/bin/vc100/Win32/admemoryanalysis</outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
